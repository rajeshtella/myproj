<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
 http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
  <icon />
  <servlet>
    <servlet-name>addgoal</servlet-name>
    <servlet-class>com.login.AddGoalServlet</servlet-class>
  </servlet>
  
	<servlet-mapping>
	    <servlet-name>addgoal</servlet-name>
	    <url-pattern>/addgoal/*</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
    <servlet-name>addusecase</servlet-name>
    <servlet-class>com.login.AddUsecaseServlet</servlet-class>
  </servlet>
  
	<servlet-mapping>
	    <servlet-name>addusecase</servlet-name>
	    <url-pattern>/addusecase/*</url-pattern>
  	</servlet-mapping>
  	
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.login.RegisterServlet</servlet-class>
  </servlet>
  
	<servlet-mapping>
	    <servlet-name>register</servlet-name>
	    <url-pattern>/register/*</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.login.LoginActionServlet</servlet-class>
  </servlet>
  
	<servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/login/*</url-pattern>
  	</servlet-mapping>
  	<servlet>
    <servlet-name>Similar Degree</servlet-name>
    <servlet-class>com.login.SimilarDegreeServlet</servlet-class>
  </servlet>
  

	<servlet-mapping>
	    <servlet-name>Similar Degree</servlet-name>
	    <url-pattern>../similardegree/*</url-pattern>
  	</servlet-mapping>
  	
  <welcome-file-list>  
   <welcome-file>.
   /login.jsp</welcome-file>  
  </welcome-file-list>  
</web-app>
